generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Player {
  id      Int    @id @default(autoincrement())
  name    String
  imageId Int
  image   Image  @relation(fields: [imageId], references: [id])
}

model Image {
  id          Int      @id @default(autoincrement())
  url         String
  path        String
  coordTop    Int
  coordLeft   Int
  coordHeight Int
  coordWidth  Int
  Player      Player[]
}
